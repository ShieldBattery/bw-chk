    "starcraft"
  ],
  "author": "Markus Heikkinen <ittevien@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bl": "^4.0.3",
    "iconv-lite": "^0.6.2"
  },
  "devDependencies": {
    "async": "^3.2.0",
    "babel-cli": "^6.18.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-tape-runner": "^2.0.1",
    "copyfiles": "^2.4.1",
    "eslint": "^7.16.0",
    "glob": "^7.1.6",
    "pngjs": "^6.0.0",
    "rimraf": "^3.0.2",
    "scm-extractor": "^1.0.4",
    "tape": "^4.13.3"
