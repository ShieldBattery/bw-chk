{
  "name": "bw-chk",
  "version": "1.1.0",
  "description": "Parses uncompressed Broodwar map files (scenario.chk).",
  "main": "lib/index.js",
  "engines": {
    "node": ">= 12.0.0"
  },
  "scripts": {
    "build": "babel --out-dir lib *.js && copyfiles units.json sprites.json lib",
    "clean": "rimraf lib",
    "lint": "eslint .",
    "prepublishOnly": "npm run lint  && npm run clean && npm run test && npm run build",
    "test": "babel-tape-runner ./test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/neivv/bw-chk"
  },
  "keywords": [
